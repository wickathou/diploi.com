{
  "name": "diploi.com",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "pnpm": {
    "overrides": {
      "isomorphic-dompurify>jsdom": "26.1.0"
    }
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.6.1",
    "@phosphor-icons/react": "^2.1.10",
    "@playform/inline": "^0.1.2",
    "@react-spring/web": "^9.7.5",
    "@sentry/astro": "^10.33.0",
    "@types/d3": "^7.4.3",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "astro": "5.16.9",
    "astro-llms-generate": "^1.3.3",
    "d3": "^7.9.0",
    "github-markdown-css": "^5.8.1",
    "isomorphic-dompurify": "^2.35.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-use-measure": "^2.1.7",
    "sass": "^1.97.2",
    "simplex-noise": "^4.0.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "printWidth": 140,
    "trailingComma": "es5",
    "singleQuote": true,
    "importOrder": [
      "^react$",
      "^react/(.*)$",
      "<THIRD_PARTY_MODULES>",
      "@phosphor-icons",
      "@phosphor-icons/react",
      "^[./]"
    ],
    "importOrderCaseSensitive": true,
    "importOrderTypeScriptVersion": "5.9.3",
    "plugins": [
      "prettier-plugin-astro",
      "@ianvs/prettier-plugin-sort-imports"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  },
  "packageManager": "pnpm@10.28.0"
}
